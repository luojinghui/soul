{
  "name": "soul",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev && craco start",
    "server": "node server.js",
    "server:dev": "cross-env NODE_ENV=developer && pm2 start server.js --name soul-dev-server -o ./logs/out.log -e ./logs/error.log",
    "server:prd": "cross-env NODE_ENV=production && pm2 start server.js --name soul-prd-server -o ./logs/out.log -e ./logs/error.log",
    "build:dev": "cross-env REACT_APP_ENV=developer && craco build",
    "build:prd": "cross-env REACT_APP_ENV=production && craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^4.21.0",
    "antd-img-crop": "^4.2.3",
    "antd-mobile": "^5.19.0",
    "highlight.js": "^11.5.1",
    "markdown-it": "^13.0.1",
    "react": "^18.1.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.1.0",
    "react-draggable": "^4.4.5",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.4",
    "socket.io-client": "^4.5.1",
    "typescript": "^4.4.2",
    "url-regex": "^5.0.0",
    "web-vitals": "^2.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "NeteaseCloudMusicApi": "^4.6.6",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "mkdirp": "^1.0.4",
    "mongoose": "^6.4.0",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.0",
    "socket.io": "^4.5.1",
    "uuid": "^8.3.2",
    "vconsole": "^3.14.6"
  }
}
